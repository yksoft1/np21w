.c.obj:
	$(CC) $(CFLAGS) -c -Fo$@ $<

.cpp.obj:
	$(CXX) $(CXXFLAGS) -c -Fo$@ $<

.rc.res: 
	$(WINDRES) -fo $@ $< 
	
clean:
	del $(TARGET) $(OBJS) 
	del $(OBJS2)
	del $(OBJS_IA32)
	del $(OBJS_W9X)
	del $(OBJS_Z)
	del *.lib *.exp